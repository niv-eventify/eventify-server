%h1 New Landing Page

- semantic_form_for(@landing_page, :live_validations => false, :url => admin_landing_pages_path, :html => {:multipart => true}) do |f|
  = f.object.errors.on(:base)

  != render :partial => "form", :locals => { :f => f }

  != f.submit "Save"
  != link_to _("Cancel"), admin_landing_pages_path
