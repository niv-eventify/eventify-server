.content-wrapper
  .content-wrapper-top
    != will_paginate(collection, :renderer => RemoteDesignsWillPaginate)
    .clear

  .image-boxes-h
    - collection.in_groups_of(3).each do |group|
      .image-boxes-h-row
        - group.compact.each do |d|
          .img-box
            != render(:partial => "designs/selector", :locals => {:event => @event, :design => d})
