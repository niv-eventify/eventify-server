.title_holder{:style => title_position_css(event)}
  .title
    != (h(event.invitation_title) || "").gsub("\n", "<BR/>").gsub(" ", "&nbsp;")
.msg_holder{:style => message_position_css(event)}
  .msg
    != (h(event.guest_message) || "").gsub("\n", "<BR/>").gsub(" ", "&nbsp;")
- set_windows(event, 1)
