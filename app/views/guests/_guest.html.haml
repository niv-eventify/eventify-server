%tr[guest]
  %td.t-col-1
    .cell-bg
      %input#name-1.input-check{ :type => "checkbox" }
      %label{ :for => "name-1" }= guest.name
  %td.t-col-2
    .cell-bg
      != mail_to guest.email, guest.email
  %td.t-col-3
    .cell-bg= guest.mobile_phone
  %td.t-col-4
    .cell-bg
      %input.input-check{ :type => "checkbox" }
  %td.t-col-5
    .cell-bg
      %input.input-check{ :type => "checkbox" }
  %td.t-col-6
    .cell-bg
      %input.input-check{ :type => "checkbox" }
  %td.last
    .cell-bg
      %a.bin{ :href => "#" }
        del
